{
  "name": "co-lab-history",
  "description": "App for students looking for someone to work on a project with, who just want to learn more about whoever they were paired with for a project, and/or for professors when deciding who to pair up/group. ",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Evan Lin <elin@middlebury.edu>",
    "Seunghwan Oh <seunghwano@middlebury.edu>",
    "Andrew Kriter <akriter@middlebury.edu>",
    "Oliscia Thornton <okthornton@middlebury.edu>",
    "Cameron Hudson <chudson@middlebury.edu>",
    "Dante Aguilar <daguilar@middlebury.edu",
    "Tara Masri <tmasri@middlebury.edu>"
  ],
  "scripts": {
    "predev": "docker compose -f docker-compose.dev.yml up -d",
    "postdev": "docker compose -f docker-compose.dev.yml down",
    "dev": "node ./dev-runner.js next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint src",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "ajv-formats": "^3.0.1",
    "connect": "^3.7.0",
    "db-errors": "^0.2.3",
    "knex": "^3.1.0",
    "next": "^13.5.6",
    "next-connect": "^1.0.0",
    "objection": "^3.1.5",
    "pg": "^8.13.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sqlite3": "^5.1.7"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,jsx}": "prettier --write"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.13.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "testcontainers": "^10.13.2"
  }
}
